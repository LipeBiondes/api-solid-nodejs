{
  "name": "api-solid-nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "npm run services:up && npm run migrations:up && tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "npx eslint src/**/*.ts --fix",
    "format": "npx prettier --write src/**/*.ts",
    "lint:all": "npm run format && npm run lint",
    "lint:check": "npx eslint src/**/*.ts",
    "format:check": "npx prettier --check src/**/*.ts",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:down": "docker compose -f infra/compose.yaml down",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "migrations:create": "npx prisma migrate dev",
    "migrations:up": "npx prisma migrate deploy"
  },
  "keywords": [],
  "author": "alefe",
  "license": "MIT",
  "description": "Uma api usando conceitos solid com nodejs",
  "devDependencies": {
    "@eslint/js": "9.19.0",
    "@types/node": "22.10.7",
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "@vitest/coverage-v8": "3.0.4",
    "@vitest/ui": "3.0.4",
    "eslint": "9.19.0",
    "globals": "15.14.0",
    "prettier": "3.4.2",
    "prisma": "6.2.1",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.22.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.4"
  },
  "dependencies": {
    "@fastify/jwt": "9.0.3",
    "@prisma/client": "6.2.1",
    "@types/bcryptjs": "2.4.6",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "fastify": "5.2.1",
    "zod": "3.24.1"
  }
}
